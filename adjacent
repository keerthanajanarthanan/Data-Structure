import java.util.*;
public class Main 
{
    public static void main(String [] args)
    {
        Scanner sc=new Scanner(System.in);
        String CE=sc.nextLine();
        String result=" ";
        for(int i=0;i<CE.length();i++)
        {
        char ch = CE.charAt(i);
        if(result==" "|| ch !=result.charAt(result.length()-1))
        {
            result+=ch;
        }
        }
        System.out.print(result);
    }
}
